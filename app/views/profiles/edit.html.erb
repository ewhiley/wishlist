<h1>Edit Profile</h1>

 <%= form_with model: @profile, local: true  do |form| %>
  <p><%= form.label :name %>
  <%= form.text_field :name %></p>
  <p><%= form.label :phone_no %>:
  <%= form.telephone_field :phone_no %></p>
  <p><%= form.label :street_number %>
  <%= form.text_field :street_number %>
  <p><%= form.label :street_address %>
  <%= form.text_field :street_address %></p>
  <p><%= form.label :suburb %>
  <%= form.text_field :suburb %></p>
  <%= form.label :state %>
  <%= form.select :state, "<option>New South Wales</option>Victoria<option>Queensland</option><option>Western Australia</option><option>South Australia</option><option>Tasmania</option><option>Australian Capital Territory</option><option>Northern Territory</option>".html_safe  %>
  <p><%= form.label :postcode %>
  <%= form.text_field :postcode %></p>
  <%#= form.text_field :category_ids, multiple: true %>
  <%= form.submit %>
<% end %>